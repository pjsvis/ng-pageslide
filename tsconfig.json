{
    "compilerOptions": {
        "module": "system",
        "target": "es5",
        "noImplicitAny": true,
        "outFile": "dist/app.min.js",
        "sourceMap": true,
        "noEmitOnError": true ,
        "noResolve": true,
        "newLine": "CRLF"   
    },
    "include":[
        "./examples/app.ts",
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "node_modules/@types",
        "typings"
    ]
}